{
  "name": "medical",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app:start": "node ./dist/",
    "start": "rimraf dist && npm run tslint && copyfiles -u 2 src/email-templates/*.hbs dist/email-templates/ && tsc-watch --outDir ./dist --onSuccess \"npm run app:start\" --onFailure \"'echo Compilation Failed'\"",
    "cron:start": "node ./dist/cron-job/cron-run",
    "cron:dev": "rimraf dist/cron && npm run tslint && tsc-watch --outDir ./dist --onSuccess \"npm run cron:start\" --onFailure \"'echo Compilation Failed'\"",
    "build": "rimraf dist && tsc && copyfiles -u 2 src/email-templates/*.hbs dist/email-templates/",
    "tsc": "rimraf dist && npm run tslint && tsc --watch",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "migrate:up": "npm run build && npx sequelize-cli db:migrate",
    "migrate:down": "npm run build && npx sequelize-cli db:migrate:undo",
    "create:db": "node_modules/.bin/sequelize db:create",
    "first": "sequelize db:create && npx sequelize-cli db:migrate"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "latest",
    "fs-extra": "^8.1.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.2.1",
    "sequelize": "latest",
    "sequelize-cli": "^5.5.1",
    "uuid": "latest",
    "dayjs": "^1.8.15",
    "dotenv": "^6.1.1",
    "email-templates": "^6.0.0",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.20.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "morgan": "^1.9.1",
    "chai-http": "^4.3.0",
    "@types/joi": "^14.3.3",
    "@types/express-rate-limit": "^3.3.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/helmet": "^0.0.44",
    "@types/morgan": "^1.7.37",
    "@types/fs-extra": "^8.0.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/sequelize": "^4.28.5",
    "copyfiles": "^2.1.1",
    "eslint": "^6.1.0",
    "husky": "^3.0.1",
    "rimraf": "^2.6.3",
    "tsc-watch": "^2.2.1",
    "tslint": "^5.18.0",
    "tslint-config-security": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.3"
  },
